{
    "name": "yceruto/formflow-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "description": "Create and manage multistep forms",
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.3",
        "symfony/form": "^6.4|^7.0",
        "symfony/framework-bundle": "^6.4|^7.0",
        "symfony/validator": "^6.4|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11",
        "symfony/browser-kit": "^7.3",
        "symfony/css-selector": "^7.3",
        "symfony/translation": "^7.3",
        "symfony/twig-bundle": "^7.3",
        "symfony/web-profiler-bundle": "^7.3",
        "symfony/yaml": "^7.3"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Yceruto\\FormFlowBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yceruto\\FormFlowBundle\\Tests\\": "tests/"
        }
    }
}
